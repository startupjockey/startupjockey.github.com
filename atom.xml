---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>StartupJockey</title>
 <link href="http://startupjockey.github.com/atom.xml" rel="self"/>
 <link href="http://startupjockey.github.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://startupjockey.github.com/</id>
 <author>
   <name>StartupJockey</name>
   <email>Startup.Jockey@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="{{site.baseurl}}{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>{{site.baseurl}}{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>